pythonaddmod() {
    mkdir $1 && touch "$1/__init__.py"
}

# Ssh Agent start
sas() {
  eval $(ssh-agent)
  ssh-add ~/.ssh/work
  ssh-add ~/.ssh/personal
}

gpush() {
    git push origin $(git rev-parse --abbrev-ref HEAD)
}

coa() {
    git co AXPT-$1
}
